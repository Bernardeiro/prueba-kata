{
    "name": "deg540/prueba-kata-php",
    "description": "Test of a kata made in PHP",
    "type": "project",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.10"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Deg540\\PruebaKataPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deg540\\PruebaKataPHP\\Test\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "test": [
            "phpunit --configuration phpunit.xml"
        ]
    }
}
